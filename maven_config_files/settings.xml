    <profiles>
        <!--
            this is maven's base profile and it's better be the first one in profile lists to get the highest priority
        -->
        <profile>
            <id>artifactory-tapsell</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <repositories>
                <repository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>tapsell-release-repo</id>
                    <name>tapsell-libs</name>
                    <url>https://artifactory.pegah.tech/artifactory/tapsell-libs</url>
                </repository>
                <repository>
                    <snapshots/>
                    <id>tapsell-snapshot-repo</id>
                    <name>tapsell-libs</name>
                    <url>https://artifactory.pegah.tech/artifactory/tapsell-libs</url>
                </repository>
                <repository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>pegah-shared-release</id>
                    <name>tapsell</name>
                    <url>https://artifactory.pegah.tech/artifactory/tapsell</url>
                </repository>
                <repository>
                    <snapshots/>
                    <id>pegah-shared-snapshot</id>
                    <name>tapsell</name>
                    <url>https://artifactory.pegah.tech/artifactory/tapsell</url>
